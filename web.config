<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <remove fileExtension=".htm" />
            <remove fileExtension=".html" />
            <mimeMap fileExtension=".html" mimeType="text/html;charset=ISO-8859-15" />
            <mimeMap fileExtension=".htm" mimeType="text/html;charset=ISO-8859-15" />
        </staticContent>
    </system.webServer>
</configuration>
